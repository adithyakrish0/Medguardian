{
  "name": "medguardian-mobile",
  "version": "1.0.0",
  "description": "MedGuardian Mobile App - Medication Management System",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.2",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.5",
    "expo": "~48.0.15",
    "expo-camera": "^13.2.0",
    "expo-constants": "~14.2.1",
    "expo-font": "~11.1.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-location": "~15.1.1",
    "expo-notifications": "~0.16.1",
    "expo-permissions": "~14.0.5",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.3",
    "firebase": "^9.15.0",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.9.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.1",
    "@types/react": "~18.0.27",
    "@types/react-test-renderer": "^18.0.0",
    "eslint": "^8.39.0",
    "eslint-config-expo": "^12.0.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.4"
  },
  "private": true,
  "keywords": [
    "medguardian",
    "medication",
    "reminder",
    "health",
    "mobile",
    "react-native",
    "expo",
    "typescript"
  ],
  "author": "MedGuardian Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/medguardian-mobile.git"
  }
}
